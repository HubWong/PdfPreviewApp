<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Pdf 管理</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css"  asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/site.css"  asp-append-version="true"/>
  
    @await RenderSectionAsync("Styles",false) 
</head>
<body>


    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow">
            <div class="container">
                <a class="navbar-brand" href="/">PDF</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">首页</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Dashboard" asp-action="index">PDF管理</a>
                        </li>                       
                         <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="privacy">关于</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
   
    <div class="container-fluid p-0">
        @RenderBody()
    </div>     
     <script src="~/lib/jquery/dist/jquery.min.js"></script> 
     <script src="~/lib/bootstrap/js/bootstrap.js"></script>
     <script src="~/lib/artDialog/dist/dialog-plus.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
