        "themes/bootstrap-table/fonts/bootstrap-table.ttf",
        "themes/bootstrap-table/fonts/bootstrap-table.woff",
        "themes/bootstrap-table/bootstrap-table.css",
        "themes/bootstrap-table/bootstrap-table.js",
        "themes/bootstrap-table/bootstrap-table.min.css",
        "themes/bootstrap-table/bootstrap-table.min.js",
