        //"themes/semantic/bootstrap-table-semantic.min.css",
        //"themes/semantic/bootstrap-table-semantic.min.js",
        "bootstrap-table-locale-all.min.js",
        "bootstrap-table.min.css"
      ]
    },
    {
      "library": "jquery-treetable@3.2.0",
      "destination": "wwwroot/lib/jquery-treetable/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap-treeview-2@1.2.1",
      "destination": "wwwroot/bootstrap-treeview-2/",
