        //"themes/bulma/bootstrap-table-bulma.min.css",
        //"themes/bulma/bootstrap-table-bulma.min.js",
        //"themes/foundation/bootstrap-table-foundation.css",
        //"themes/foundation/bootstrap-table-foundation.js",
